    <xsd:complexType name="AgentType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Any entity that is able to carry out actions.</xsd:documentation>
        </xsd:annotation>
       <xsd:sequence>
            <xsd:element ref="AgentAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="ConstraintType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A limitation applied to Requirement(s) or to the Information Concept(s) the Requirement is about.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="RequirementType">
            	<xsd:sequence>
            		<xsd:element ref="ConstraintAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
            	</xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="CriterionType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A condition for evaluation or assessment.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="RequirementType">
                <xsd:sequence>
                    <xsd:element ref="Bias" minOccurs="0" maxOccurs="unbounded">
                    	<xsd:annotation>
            				<xsd:documentation xml:lang="en">Additional parameter(s) used to adjust the evaluation of the Criterion composed of multiple sub-Criteria and their respective weights.</xsd:documentation>
        				</xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="Weight" minOccurs="0" maxOccurs="unbounded">
                    	<xsd:annotation>
            				<xsd:documentation xml:lang="en">Relative importance assigned to a given Criterion.</xsd:documentation>
        				</xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="WeightingConsiderationDescription" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
            				<xsd:documentation xml:lang="en">The explanation of how the weighting of a Criterion is to be used at evaluation time.</xsd:documentation>
        				</xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="WeightingTypeProperty" minOccurs="0" maxOccurs="unbounded">
                       	<xsd:annotation>
            				<xsd:documentation xml:lang="en">The numeric form used to weight.</xsd:documentation>
        				</xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="CriterionAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="DatasetType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A collection of data.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
            	<xsd:annotation>
            		<xsd:documentation xml:lang="en">An unambiguous reference to a Dataset.</xsd:documentation>
        		</xsd:annotation>
            </xsd:element>
            <xsd:element ref="DatasetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="EvidenceType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The data proving that a Requirement is met or has been fulfilled.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="DatasetType">
                <xsd:sequence>
                    <xsd:element ref="ConfidentialityLevelTypeProperty" minOccurs="0" maxOccurs="unbounded">
            			<xsd:annotation>
            				<xsd:documentation xml:lang="en">The security classification assigned to an Evidence or, in general, to a content.</xsd:documentation>
        				</xsd:annotation>
                    </xsd:element>
                    <xsd:element ref="IsAbout" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsConformantTo" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsCreatedBy" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="EvidenceIsIssuedBy" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SupportsRequirement" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SupportsValue" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="ValidityPeriod" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="EvidenceAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="EvidenceTypeType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Information about the characteristics of an expected Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="EvidenceTypeClassification" minOccurs="0" maxOccurs="unbounded">
           		<xsd:annotation>
           			<xsd:documentation xml:lang="en">A classification code to specify the layout and content expected for an Evidence.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
           		<xsd:annotation>
           			<xsd:documentation xml:lang="en">An unambiguous reference to an Evidence Type.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="IsSpecifiedIn" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="ValidityPeriodConstraint" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="EvidenceTypeAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="EvidenceTypeListType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A combination of Evidence Types for each of which a conforming Evidence must be provided.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="Description" minOccurs="0" maxOccurs="unbounded">
         		<xsd:annotation>
         			<xsd:documentation xml:lang="en">A short explanation about the nature, attributes, uses or any other additional information that helps clarify the understanding of the Evidence Type being instantiated.</xsd:documentation>
     			</xsd:annotation>           
            </xsd:element>
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">An unambiguous reference to an Evidence Type List.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="Name" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">The name of the Evidence Type List.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="SpecifiesEvidenceTypeAssociation" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="EvidenceTypeListAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="InformationConceptType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A reference to an entity, i.e. a class or a property, which is used to describe information in the Evidence to be provided for the Requirement specified.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="Description" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">A short explanation about the nature, attributes, uses or any other additional information that helps clarify the understanding of the Information Concept being instantiated.</xsd:documentation>
       			</xsd:annotation>            
            </xsd:element>
            <xsd:element ref="ExpressionOfExpectedValue" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">An expression (function) of the value that the Requirement expects.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">An unambiguous reference to the Information Concept.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="Name" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
           			<xsd:documentation xml:lang="en">A name of the Information Concept.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
            <xsd:element ref="TypeProperty" minOccurs="0" maxOccurs="unbounded">
                 <xsd:annotation>
           			<xsd:documentation xml:lang="en">A classification of the Information Concept.</xsd:documentation>
       			</xsd:annotation>
            </xsd:element>
           <xsd:element ref="InformationConceptAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="InformationRequirementType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A request for data that is proof of Evidence or that leads to the source of such a proof.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="RequirementType">
	            <xsd:sequence>
	            	<xsd:element ref="InformationRequirementAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
	            </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="LocationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An identifiable geographic place or named place.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
	   		<xsd:element ref="LocationAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
	    </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="PeriodOfTimeType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A temporal entity with non-zero extent or duration, i.e. for which the value of the beginning and end are different.</xsd:documentation>
        </xsd:annotation> 
        <xsd:sequence>
            <xsd:element ref="PeriodOfTime" minOccurs="0" maxOccurs="unbounded">
		        <xsd:annotation>
		            <xsd:documentation xml:lang="en">Extent of the Period of Time.</xsd:documentation>
		        </xsd:annotation>            
            </xsd:element>
            <xsd:element ref="EndTime" minOccurs="0" maxOccurs="unbounded">
            	<xsd:annotation>
		            <xsd:documentation xml:lang="en">Date and time information indicating the end of the Period.</xsd:documentation>
		        </xsd:annotation> 
            </xsd:element>
            <xsd:element ref="StartTime" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">Date and time information indicating the beginning of the Period.</xsd:documentation>
		        </xsd:annotation> 
            </xsd:element>
            <xsd:element ref="PeriodOfTimeAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="ReferenceFrameworkType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A source from where Requirements are identified and derived.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">An unambiguous reference to a Reference Framework.</xsd:documentation>
		        </xsd:annotation> 
            </xsd:element>
            <xsd:element ref="ReferenceFrameworkAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="RequirementType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A condition or prerequisite that someone requests and someone else has to meet.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="Description" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">A short explanation about the nature, attributes, uses or any other additional information that helps clarify the understanding of the Requirement being instantiated.</xsd:documentation>
		        </xsd:annotation>            	
            </xsd:element>
            <xsd:element ref="HasQualifiedRelation"  minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="HasRequirement" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="Identifier" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">An unambiguous reference to a Requirement.</xsd:documentation>
		        </xsd:annotation>             	
            </xsd:element>
            <xsd:element ref="IsRequirementOf" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="RequirementIsIssuedBy " minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="Name" minOccurs="0" maxOccurs="unbounded">
            	<xsd:annotation>
		            <xsd:documentation xml:lang="en">A name to identify in common language the Requirement.</xsd:documentation>
		        </xsd:annotation>   
		    </xsd:element>
            <xsd:element ref="TypeProperty" minOccurs="0" maxOccurs="unbounded">
            	<xsd:annotation>
		            <xsd:documentation xml:lang="en">The category or type to which the requirement belongs.</xsd:documentation>
		        </xsd:annotation>
            </xsd:element>
            <xsd:element ref="RequirementAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="SupportedValueType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A value for an Information Concept that is supported by an Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
        	<xsd:element ref="ProvidesValueFor" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element ref="Query" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">The query via which the value for the Information Concept can be retrieved. The query must be executed on the business data provided by the supporting Evidence.</xsd:documentation>
		        </xsd:annotation>
            </xsd:element>
            <xsd:element ref="Value" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
		            <xsd:documentation xml:lang="en">The value for the Information Concept that the Evidence is supporting..</xsd:documentation>
		        </xsd:annotation>            
            </xsd:element>
            <xsd:element ref="SupportedValueAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute ref="id" />
    </xsd:complexType>

    <xsd:complexType name="ConstrainsAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Information Concept about which a Constraint expresses a limitation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="InformationConcept" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element ref="ConstrainsAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="InformationConceptRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="EvidenceIsIssuedByAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Agent that produces the Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Agent" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="EvidenceIsIssuedByAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="AgentRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="HasConceptAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Information Concept for which the Requirement expects a support in the supporting Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                 <xsd:sequence>
                    <xsd:element ref="InformationConcept" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="HasConceptAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="InformationConceptRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="HasEvidenceTypeListAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The single or various combinations of Evidence Types for supporting a Requirement. Out of the different Lists, one of them must be satisfied by the response to the Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                 <xsd:sequence>
                    <xsd:element ref="EvidenceTypeList" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="HasEvidenceTypeListAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="EvidenceTypeListRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="HasQualifiedRelationAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A described and/or categorised relation to the instance of another Requirement class or subclass.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Requirement" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="HasQualifiedRelationAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="RequirementRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="HasRequirementAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A sub-Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Requirement" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="HasRequirementAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="RequirementRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="HasSupportingEvidenceAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Evidence that supplies information (proof, support) for this Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Evidence" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="HasSupportingEvidenceAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="EvidenceRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="IsAboutAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Agent that is the main actor in the provided Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Agent" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsAboutAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="AgentRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

   <xsd:complexType name="IsConformantToAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A reference to one or more types of Evidence where the content and, possibly, the layout or configuration of the Evidence is specified.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="EvidenceTypeClass" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsConformantToAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="EvidenceTypeRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="IsCreatedByAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Agent that produces the Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Agent" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsCreatedByAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="AgentRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="IsDerivedFromAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Reference Framework that is responsible for the creation/initiation of the Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="ReferenceFramework" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsDerivedFromAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="ReferenceFrameworkRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="IsRequirementOfAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A reference between a sub-Requirement and its parent, more general, Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
            	<xsd:sequence>
                    <xsd:element ref="Requirement" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsRequirementOfAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="RequirementRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="IsSpecifiedInAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Refers to an Evidence Type List in which the Evidence Type is included.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="EvidenceTypeList" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="IsSpecifiedInAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="EvidenceTypeListRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProvidesValueForAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Information Concept for which the Supported Value is providing a value.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="InformationConcept" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="ProvidesValueForAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="InformationConceptRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RequirementIsIssuedByAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Agent that has published the Requirement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
            	<xsd:sequence>
                    <xsd:element ref="Agent" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="RequirementIsIssuedByAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="AgentRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SpecifiesEvidenceTypeAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Indicates one Type of Evidence included in a List of Evidence Types.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="EvidenceTypeClass" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SpecifiesEvidenceTypeAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="EvidenceTypeRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SupportsConceptAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Information Concept that is supported by the Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="InformationConcept" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SupportsConceptAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="InformationConceptRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="SupportsRequirementAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Requirement for which the Evidence provides information (proof, support).</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="Requirement" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SupportsRequirementAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="RequirementRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="SupportsValueAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The Supported Value for which an Evidence holds information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="SupportedValue" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="SupportsValueAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="SupportedValueRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ValidityPeriodAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Indicates the validity period of the Evidence.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="PeriodOfTime" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="ValidityPeriodAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="PeriodOfTimeRef"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ValidityPeriodConstraintAssociationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Indicates a temporal constraint on the validity period of the Evidence Type.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="AssociationType">
                <xsd:sequence>
                    <xsd:element ref="PeriodOfTime" minOccurs="0" maxOccurs="unbounded" />
                    <xsd:element ref="ValidityPeriodConstraintAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute ref="PeriodOfTimeRef" use="optional"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:attribute name="id" type="xsd:ID">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A document-relative identifier for an XML element.</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>

    <xsd:attribute name="uri" type="xsd:anyURI">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An internationalized resource identifier or uniform resource identifier for a node or object.</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>

    <xsd:attribute name="languageID" type="xsd:language">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The language of the type</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>

    <xsd:simpleType name="LiteralSimpleType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A data type for a thing with its own lifespan that has some existence.</xsd:documentation>
        </xsd:annotation>
        <xsd:union memberTypes="xsd:int xsd:string" />
    </xsd:simpleType>

    <xsd:complexType name="TextType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A character string (i.e. a finite set of characters) generally in the form of words of a language</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute ref="languageID" />
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="CodeType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A character string (letters, figures, or symbols) that for brevity and/or languange independence may be used to represent or replace a definitive value or text of an attribute together with relevant supplementary information.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xsd:normalizedString">
                <xsd:attribute ref="uri" />
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="AssociationType" abstract="true">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A data type for a relationship between two or more objects, including any properties of that relationship.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
      		<xsd:element ref="AssociationAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
   		</xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="AssociationAugmentationPoint" abstract="true">
    	<xsd:annotation>
    	    <xsd:documentation xml:lang="en">An augmentation point for type structures:AssociationType.</xsd:documentation>
    	</xsd:annotation>
